pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Building the Application'
            }
        }
         stage('Deploy') {
            steps {
                echo 'Deploying the Application'
            }
        }
         stage('Test') {
            steps {
                echo 'Testing the Application'
            }
        }
         stage('Release') {
            steps {
                echo 'Releasing the Application'
            }
        }
    }
}
